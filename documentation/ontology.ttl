                      rdfs:label "has document reference" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasFile
:hasFile rdf:type owl:ObjectProperty ;
         rdfs:comment "Link a topic with internal or external files such as BIM models, videos, images, text, etc."@en ;
         rdfs:label "has file" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasFormat
:hasFormat rdf:type owl:ObjectProperty ;
           rdfs:label "has format" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasLine
:hasLine rdf:type owl:ObjectProperty ;
         rdfs:comment "Connects a viewpoint with a line being used to add markups in 3D."@en ;
         rdfs:label "has line" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasLocation
:hasLocation rdf:type owl:ObjectProperty ;
             rdfs:comment "Indicates the location of the bitmap, specifically the center of the bitmap in 3D world coordinates."@en ;
             rdfs:label "has location" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasPoint
:hasPoint rdf:type owl:ObjectProperty ;
          rdfs:comment "Relates to an specific point in 3D world coordinates."@en ;
          rdfs:label "has point" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasReferenceLink
:hasReferenceLink rdf:type owl:ObjectProperty ;
                  rdfs:comment "External references that help complemente the topic in discussion."@en ;
                  rdfs:label "has reference link" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasStage
:hasStage rdf:type owl:ObjectProperty ;
          rdfs:comment "Relates a topic with its stage."@en ;
          rdfs:label "has stage" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicLabel
:hasTopicLabel rdf:type owl:ObjectProperty ;
               rdfs:comment "Relates a topic with its label."@en ;
               rdfs:label "has topic label" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicStatus
:hasTopicStatus rdf:type owl:ObjectProperty ;
                rdfs:comment "Relates a topic with its status."@en ;
                rdfs:label "has topic status" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicType
:hasTopicType rdf:type owl:ObjectProperty ;
              rdfs:comment "Relates a topic with its type."@en ;
              rdfs:label "has topic type" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasViewpoint
:hasViewpoint rdf:type owl:ObjectProperty ;
              rdfs:comment "Relates a comment with a viewpoint."@en ;
              rdfs:label "has viewpoint" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#normalVector
:normalVector rdf:type owl:ObjectProperty ;
              rdfs:comment "Indicates the normal direction of the bitmap."@en ;
              rdfs:label "normal vector" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToBuilding
:relatedToBuilding rdf:type owl:ObjectProperty ;
                   rdfs:comment "Relates a topic to a building."@en ;
                   rdfs:label "related to building"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToComment
:relatedToComment rdf:type owl:ObjectProperty ;
