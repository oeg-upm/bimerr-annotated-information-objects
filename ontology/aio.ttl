#################################################################
#    Classes
#################################################################

###  http://bimerr.iot.linkeddata.es/def/annotated-information#BimFormat
aio:BimFormat rdf:type owl:Class ;
              rdfs:subClassOf aio:Format ;
              rdfs:comment "Concept that describes BIM file formats"@en ;
              rdfs:label "Bim format"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Bitmap
aio:Bitmap rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasLocation ;
                             owl:someValuesFrom aio:Coordinate3D
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:normalVector ;
                             owl:someValuesFrom aio:Coordinate3D
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:upVector ;
                             owl:someValuesFrom aio:Coordinate3D
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasFormat ;
                             owl:allValuesFrom aio:BitmapFormat
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:height ;
                             owl:someValuesFrom xsd:float
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:reference ;
                             owl:someValuesFrom xsd:string
                           ] ;
           rdfs:comment "A list of bitmaps can be used to add more information, for example, text in the visualization."@en ;
           rdfs:label "Bitmap"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#BitmapFormat
aio:BitmapFormat rdf:type owl:Class ;
                 rdfs:subClassOf aio:Format ;
                 rdfs:comment "Concept that describe Bitmap file formats."@en ;
                 rdfs:label "Bitmap format"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#ClippingPlane
aio:ClippingPlane rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty aio:hasDirection ;
                                    owl:someValuesFrom aio:Coordinate3D
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty aio:hasLocation ;
                                    owl:someValuesFrom aio:Coordinate3D
                                  ] ;
                  rdfs:comment "ClippingPlanes can be used to define a subsection of a building model that is related to the topic. Each clipping plane is defined by Location and Direction."@en ;
                  rdfs:label "Clipping plane"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Comment
aio:Comment rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty aio:underViewpoint ;
                              owl:allValuesFrom aio:Viewpoint
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty aio:comment ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty aio:creationAuthor ;
                              owl:someValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty aio:creationDate ;
                              owl:someValuesFrom xsd:dateTime
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty aio:modifiedAuthor ;
                              owl:allValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty aio:modifiedDate ;
                              owl:allValuesFrom xsd:dateTime
                            ] .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#ComponentSelection
aio:ComponentSelection rdf:type owl:Class ;
                       rdfs:subClassOf [ rdf:type owl:Restriction ;
                                         owl:onProperty aio:hasComponent ;
                                         owl:someValuesFrom building:Component
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty aio:color ;
                                         owl:allValuesFrom xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty aio:defaultVisibility ;
                                         owl:allValuesFrom xsd:boolean
                                       ] ;
                       rdfs:comment "The Selection element lists all components that should be either highlighted or selected when displaying a viewpoint."@en ;
                       rdfs:label "Component selection"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Concept
aio:Concept rdf:type owl:Class ;
            rdfs:label "Concept"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#ConceptScheme
aio:ConceptScheme rdf:type owl:Class ;
                  rdfs:label "Concept scheme"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Coordinate3D
aio:Coordinate3D rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty aio:xCoordinate ;
                                   owl:someValuesFrom xsd:float
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty aio:yCoordinate ;
                                   owl:someValuesFrom xsd:float
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty aio:zCoordinate ;
                                   owl:someValuesFrom xsd:float
                                 ] ;
                 rdfs:comment "A 3 dimensional point."@en ;
                 rdfs:label "Coordinate 3D"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#DocumentReference
aio:DocumentReference rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty aio:reference ;
                                        owl:someValuesFrom xsd:string
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty aio:description ;
                                        owl:allValuesFrom xsd:string
                                      ] ;
                      rdfs:comment "Provides a means to associate additional payloads or links with topics. The references may point to a file within the .bcfzip or to an external location."@en ;
                      rdfs:label "Document reference"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#File
aio:File rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty aio:hasFormat ;
                           owl:allValuesFrom aio:BimFormat
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty aio:ifcSpatialStructureElement ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty aio:modifiedDate ;
                           owl:someValuesFrom xsd:dateTime
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty aio:reference ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty aio:referenceScheme ;
                           owl:someValuesFrom xsd:string
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty kpi:filename ;
                           owl:someValuesFrom xsd:string
                         ] ;
         rdfs:comment "Information node about a specific IFC file related to the topic."@en ;
         rdfs:label "File"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Format
aio:Format rdf:type owl:Class ;
           rdfs:label "Format"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Line
aio:Line rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty aio:hasPoint ;
                           owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                           owl:onClass aio:Coordinate3D
                         ] ;
         rdfs:comment "Lines used to add markup in 3D."@en ;
         rdfs:label "Line"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Markup
aio:Markup rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasBimSnippet ;
                             owl:someValuesFrom aio:File
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasFile ;
                             owl:someValuesFrom aio:File
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasTopic ;
                             owl:someValuesFrom aio:Topic
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:relatedToProject ;
                             owl:someValuesFrom kpi:Project
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasComment ;
                             owl:allValuesFrom aio:Comment
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty aio:hasViewpoint ;
                             owl:allValuesFrom aio:Viewpoint
                           ] ;
           rdfs:comment "Textual information about the topic."@en ;
           rdfs:label "Markup"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Stage
aio:Stage rdf:type owl:Class ;
          rdfs:subClassOf aio:Concept ;
          rdfs:comment "Concept that groups predefined stages of a construction project."@en ;
          rdfs:label "Stage"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#StageConceptScheme
aio:StageConceptScheme rdf:type owl:Class ;
                       rdfs:subClassOf aio:ConceptScheme ;
                       rdfs:label "Stage concept scheme"@en .


###  http://bimerr.iot.linkeddata.es/def/annotated-information#Topic
aio:Topic rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty aio:hasDocument ;
                            owl:allValuesFrom aio:DocumentReference
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:hasStage ;
                            owl:allValuesFrom aio:Stage
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:relatedToTopic ;
                            owl:allValuesFrom aio:Topic
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:topicLabel ;
                            owl:allValuesFrom aio:TopicLabelConcept
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:topicStatus ;
                            owl:allValuesFrom aio:TopicStatusConcept
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:topicType ;
                            owl:allValuesFrom aio:TopicConcept
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:creationAuthor ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:creationDate ;
                            owl:someValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:reference ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:title ;
                            owl:someValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:assignedTo ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:description ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:dueDate ;
                            owl:allValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty aio:index ;
                            owl:allValuesFrom xsd:integer
                          ] ,
