@prefix : <http://bimerr.iot.linkeddata.es/def/annotation-objects#> .
@prefix ao: <http://bimerr.iot.linkeddata.es/def/annotation-objects#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix io: <http://bimerr.iot.linkeddata.es/def/information-objects#> .
@prefix kpi: <http://bimerr.iot.linkeddata.es/def/key-performance-indicator#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix building: <http://bimerr.iot.linkeddata.es/def/building#> .
@base <http://bimerr.iot.linkeddata.es/def/annotation-objects#> .

<http://bimerr.iot.linkeddata.es/def/annotation-objects#> rdf:type owl:Ontology ;
                                                           dc:created "2020-11-01" ;
                                                           dc:creator "María Poveda-Villalón" ,
                                                                      "Serge Chávez-Feria" ;
                                                           dc:license "http://purl.org/NET/rdflicense/cc-by4.0" ;
                                                           dc:publisher "http://www.oeg-upm.net/" ;
                                                           dc:title "Annotations Ontology" ;
                                                           vann:preferredNamespacePrefix "ao" ;
                                                           rdfs:comment "The Annotation Objects ontology aims to model the annotations and comments made during a renovation project with respect to specific building elements, spaces, etc."@en ;
                                                           owl:versionInfo "0.0.4" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/created
dc:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/creator
dc:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/license
dc:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/publisher
dc:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
dc:title rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://bimerr.iot.linkeddata.es/def/annotation-objects#cameraDirection
ao:cameraDirection rdf:type owl:ObjectProperty ;
                   rdfs:domain ao:Camera ;
                   rdfs:range ao:Coordinate3D ;
                   rdfs:comment "Indicates the camera direction of the viewpoint."@en ;
                   rdfs:label "camera direction" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#cameraUpVector
ao:cameraUpVector rdf:type owl:ObjectProperty ;
                  rdfs:domain ao:Camera ;
                  rdfs:range ao:Coordinate3D ;
                  rdfs:comment "Indicates the camera up vector."@en ;
                  rdfs:label "camera up vector" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#cameraViewPoint
ao:cameraViewPoint rdf:type owl:ObjectProperty ;
                   rdfs:domain ao:Camera ;
                   rdfs:range ao:Coordinate3D ;
                   rdfs:comment "Indicates the camera viewpoint direction."@en ;
                   rdfs:label "camera view point" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasBimSnippet
ao:hasBimSnippet rdf:type owl:ObjectProperty ;
                 rdfs:comment "Explicit property to relate a topic to an specific part of the BIM Model."@en ;
                 rdfs:label "has bim snippet" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasBitmap
ao:hasBitmap rdf:type owl:ObjectProperty ;
             rdfs:domain ao:Viewpoint ;
             rdfs:range ao:Bitmap ;
             rdfs:comment "Relates a viewpoint with a bitmap element."@en ;
             rdfs:label "has bitmap" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasCamera
ao:hasCamera rdf:type owl:ObjectProperty ;
             rdfs:domain ao:Viewpoint ;
             rdfs:range ao:Camera ;
             rdfs:comment "Relates a viewpoint with the camera being used."@en ;
             rdfs:label "has camera" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasClippingPlane
ao:hasClippingPlane rdf:type owl:ObjectProperty ;
                    rdfs:domain ao:Viewpoint ;
                    rdfs:range ao:ClippingPlane ;
                    rdfs:comment "Relates a viewpoint with a clipping plane used to define a subsection in the building model."@en ;
                    rdfs:label "has clipping plane" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasComment
ao:hasComment rdf:type owl:ObjectProperty ;
              rdfs:domain ao:Topic ;
              rdfs:range ao:Comment ;
              rdfs:comment "Relates a topic with a comment."@en ;
              rdfs:label "has comment" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasComponent
ao:hasComponent rdf:type owl:ObjectProperty ;
                rdfs:domain ao:Viewpoint ;
                rdfs:range building:Element ;
                rdfs:comment "Related a viewpoint with the building elements it is conformed."@en ;
                rdfs:label "has component" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasDirection
ao:hasDirection rdf:type owl:ObjectProperty ;
                rdfs:comment "Indicates a direction given by a 3D world coordinate."@en ;
                rdfs:label "has direction" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasDocumentReference
ao:hasDocumentReference rdf:type owl:ObjectProperty ;
                        rdfs:comment "Links a topic to extra document references, such as additional payloads, or links."@en ;
                        rdfs:label "has document reference" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasFile
ao:hasFile rdf:type owl:ObjectProperty ;
           rdfs:comment "Link a topic with internal or external files such as BIM models, videos, images, text, etc."@en ;
           rdfs:label "has file" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasLine
ao:hasLine rdf:type owl:ObjectProperty ;
           rdfs:domain ao:Viewpoint ;
           rdfs:range ao:Line ;
           rdfs:comment "Connects a viewpoint with a line being used to add markups in 3D."@en ;
           rdfs:label "has line" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasLocation
ao:hasLocation rdf:type owl:ObjectProperty ;
               rdfs:comment "Indicates the location of the bitmap, specifically the center of the bitmap in 3D world coordinates."@en ;
               rdfs:label "has location" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasPoint
ao:hasPoint rdf:type owl:ObjectProperty ;
            rdfs:domain ao:Line ;
            rdfs:range ao:Coordinate3D ;
            rdfs:comment "Relates to an specific point in 3D world coordinates."@en ;
            rdfs:label "has point" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasReferenceLink
ao:hasReferenceLink rdf:type owl:ObjectProperty ;
                    rdfs:comment "External references that help complemente the topic in discussion."@en ;
                    rdfs:label "has reference link" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasStage
ao:hasStage rdf:type owl:ObjectProperty ;
            rdfs:domain ao:Topic ;
            rdfs:range ao:Stage ;
            rdfs:comment "Relates a topic with its stage."@en ;
            rdfs:label "has stage" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicLabel
ao:hasTopicLabel rdf:type owl:ObjectProperty ;
                 rdfs:domain ao:Topic ;
                 rdfs:range ao:TopicLabel ;
                 rdfs:comment "Relates a topic with its label."@en ;
                 rdfs:label "has topic label" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicStatus
ao:hasTopicStatus rdf:type owl:ObjectProperty ;
                  rdfs:domain ao:Topic ;
                  rdfs:range ao:TopicStatus ;
                  rdfs:comment "Relates a topic with its status."@en ;
                  rdfs:label "has topic status" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasTopicType
ao:hasTopicType rdf:type owl:ObjectProperty ;
                rdfs:domain ao:Topic ;
                rdfs:range ao:TopicType ;
                rdfs:comment "Relates a topic with its type."@en ;
                rdfs:label "has topic type" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#hasViewpoint
ao:hasViewpoint rdf:type owl:ObjectProperty ;
                rdfs:domain ao:Comment ;
                rdfs:range ao:Viewpoint ;
                rdfs:comment "Relates a comment with a viewpoint."@en ;
                rdfs:label "has viewpoint" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#normalVector
ao:normalVector rdf:type owl:ObjectProperty ;
                rdfs:comment "Indicates the normal direction of the bitmap."@en ;
                rdfs:label "normal vector" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToApartment
ao:relatedToApartment rdf:type owl:ObjectProperty ;
                      rdfs:domain ao:Topic ;
                      rdfs:range building:Apartment ;
                      rdfs:comment "Relates a topic with an apartment"@en ;
                      rdfs:label "related to apartment"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToBuilding
ao:relatedToBuilding rdf:type owl:ObjectProperty ;
                     rdfs:domain ao:Topic ;
                     rdfs:range building:Building ;
                     rdfs:comment "Relates a topic to a building."@en ;
                     rdfs:label "related to building"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToElement
ao:relatedToElement rdf:type owl:ObjectProperty ;
                    rdfs:domain ao:Topic ;
                    rdfs:range building:Element ;
                    rdfs:comment "Relates a topic to an element."@en ;
                    rdfs:label "related to element"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToProject
ao:relatedToProject rdf:type owl:ObjectProperty ;
                    rdfs:domain ao:Topic ;
                    rdfs:range kpi:Project ;
                    rdfs:comment "Relates a topic to a project."@en ;
                    rdfs:label "related to project" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToSpace
ao:relatedToSpace rdf:type owl:ObjectProperty ;
                  rdfs:domain ao:Topic ;
                  rdfs:range building:Space ;
                  rdfs:comment "Relates a topic to a space."@en ;
                  rdfs:label "related to space"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToStorey
ao:relatedToStorey rdf:type owl:ObjectProperty ;
                   rdfs:domain ao:Topic ;
                   rdfs:range building:Storey ;
                   rdfs:comment "Relates a topic to a storey."@en ;
                   rdfs:label "related to storey"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#relatedToTopic
ao:relatedToTopic rdf:type owl:ObjectProperty ;
                  rdfs:domain ao:Topic ;
                  rdfs:range ao:Topic ;
                  rdfs:comment "Relates a topic to another topic."@en ;
                  rdfs:label "related to topic" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#upVector
ao:upVector rdf:type owl:ObjectProperty ;
            rdfs:comment "Indicates the up direction of the bitmap."@en ;
            rdfs:label "up vector" .


#################################################################
#    Data properties
#################################################################

###  http://bimerr.iot.linkeddata.es/def/annotation-objects#assignedTo
ao:assignedTo rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "The user to whom this topic is assigned to. Recommended to be in email format."@en ;
              rdfs:label "assigned to" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#comment
ao:comment rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:string ;
           rdfs:comment "Large description of the comment."@en ;
           rdfs:label "comment" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#creationAuthor
ao:creationAuthor rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates the creator of the entity."@en ;
                  rdfs:label "creation author" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#creationDate
ao:creationDate rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "Indicates the creation date of the entity."@en ;
                rdfs:label "creation date" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#description
ao:description rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:comment "Provides a description of the topic."@en ;
               rdfs:label "description" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#dueDate
ao:dueDate rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "Date until when the topics issue needs to be resolved."@en ;
           rdfs:label "due date" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#fieldOfView
ao:fieldOfView rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:float ;
               rdfs:comment "Camera’s field of view angle in degrees."@en ;
               rdfs:label "field of view" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#identifier
ao:identifier rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:string ;
              rdfs:comment "Attribute to uniquely identify the topics, comments and viewpoints."@en ;
              rdfs:label "identifier" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#index
ao:index rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:integer ;
         rdfs:comment "Number to maintain the order of the topics."@en ;
         rdfs:label "index" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#modifiedAuthor
ao:modifiedAuthor rdf:type owl:DatatypeProperty ;
                  rdfs:range xsd:string ;
                  rdfs:comment "Indicates who modified the entity."@en ;
                  rdfs:label "modified author" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#modifiedDate
ao:modifiedDate rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:dateTime ;
                rdfs:comment "Indicates the last time the entity was modified."@en ;
                rdfs:label "modified date" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#priority
ao:priority rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:integer ;
            rdfs:comment "Topic priority."@en ;
            rdfs:label "priority" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#snapshotName
ao:snapshotName rdf:type owl:DatatypeProperty ;
                rdfs:range xsd:string ;
                rdfs:comment "Filename of the snapshot taken under a viewpoint."@en ;
                rdfs:label "snapshot name" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#title
ao:title rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         rdfs:comment "Title of the topic."@en ;
         rdfs:label "title" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#viewToWorldScale
ao:viewToWorldScale rdf:type owl:DatatypeProperty ;
                    rdfs:range xsd:float ;
                    rdfs:comment "Scaling from view to world."@en ;
                    rdfs:label "view to world scale" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#viewpointName
ao:viewpointName rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:string ;
                 rdfs:comment "Name of the viewpoint."@en ;
                 rdfs:label "viewpoint name" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#xCoordinate
ao:xCoordinate rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:float ;
               rdfs:comment "X component of a 3D coordinate."@en ;
               rdfs:label "x coordinate" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#yCoordinate
ao:yCoordinate rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:float ;
               rdfs:comment "Y component of a 3D coordinate."@en ;
               rdfs:label "y coordinate" .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#zCoordinate
ao:zCoordinate rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:float ;
               rdfs:comment "Z component of a 3D coordinate."@en ;
               rdfs:label "z coordinate" .


#################################################################
#    Classes
#################################################################

###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Bitmap
ao:Bitmap rdf:type owl:Class ;
          rdfs:subClassOf io:InformationObject ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ao:hasLocation ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ao:normalVector ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ao:upVector ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "A list of bitmaps can be used to add more information, for example, text in the visualization."@en ;
          rdfs:label "Bitmap"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Camera
ao:Camera rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty ao:cameraDirection ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ao:cameraUpVector ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty ao:cameraViewPoint ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger
                          ] ;
          rdfs:comment "Represents the type of camera being used in the viewpoint."@en ;
          rdfs:label "Camera"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#ClippingPlane
ao:ClippingPlane rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty ao:hasDirection ;
                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty ao:hasLocation ;
                                   owl:cardinality "1"^^xsd:nonNegativeInteger
                                 ] ;
                 rdfs:comment "Clipping Planes can be used to define a subsection of a building model that is related to the topic. Each clipping plane is defined by Location and Direction."@en ;
                 rdfs:label "Clipping Plane"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Comment
ao:Comment rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty ao:hasViewpoint ;
                             owl:minCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:comment ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:creationAuthor ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:creationDate ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:identifier ;
                             owl:cardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:modifiedAuthor ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty ao:modifiedDate ;
                             owl:maxCardinality "1"^^xsd:nonNegativeInteger
                           ] ;
           rdfs:comment "Their purpose is to record discussion between different parties related to the topic."@en ;
           rdfs:label "Comment"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Coordinate3D
ao:Coordinate3D rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty ao:xCoordinate ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty ao:yCoordinate ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty ao:zCoordinate ;
                                  owl:cardinality "1"^^xsd:nonNegativeInteger
                                ] ;
                rdfs:comment "Represents a coordinate in 3 dimensions."@en ;
                rdfs:label "Coordinate 3D"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Line
ao:Line rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty ao:hasPoint ;
                          owl:cardinality "2"^^xsd:nonNegativeInteger
                        ] ;
        rdfs:comment "Lines can be used to add markup in 3D. Each line is defined by three dimensional Start Point and End Point. Lines that have the same start and end points are to be considered points and may be displayed accordingly."@en ;
        rdfs:label "Line"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#OrthogonalCamera
ao:OrthogonalCamera rdf:type owl:Class ;
                    rdfs:subClassOf ao:Camera ,
                                    [ rdf:type owl:Restriction ;
                                      owl:onProperty ao:viewToWorldScale ;
                                      owl:cardinality "1"^^xsd:nonNegativeInteger
                                    ] ;
                    rdfs:comment "Represents an orthogonal camera being used in the viewpoint."@en ;
                    rdfs:label "Orthogonal Camera"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#PerspectiveCamera
ao:PerspectiveCamera rdf:type owl:Class ;
                     rdfs:subClassOf ao:Camera ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty ao:fieldOfView ;
                                       owl:cardinality "1"^^xsd:nonNegativeInteger
                                     ] ;
                     rdfs:comment "Represents an perspective camera being used in the viewpoint."@en ;
                     rdfs:label "Perspective Camera"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Stage
ao:Stage rdf:type owl:Class ;
         rdfs:comment "Stage of the topic such as Planning, Construction, etc."@en ;
         rdfs:label "Stage"@en .


###  http://bimerr.iot.linkeddata.es/def/annotation-objects#Topic
ao:Topic rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasBimSnippet ;
                           owl:allValuesFrom io:InformationObject
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasDocumentReference ;
                           owl:allValuesFrom io:InformationObject
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasFile ;
                           owl:allValuesFrom io:InformationObject
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasReferenceLink ;
                           owl:allValuesFrom io:InformationObject
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasDocumentReference ;
                           owl:minCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasFile ;
                           owl:minCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasReferenceLink ;
                           owl:minCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:relatedToProject ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasStage ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:hasTopicStatus ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:creationAuthor ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:creationDate ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:identifier ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:title ;
                           owl:cardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:assignedTo ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:description ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty ao:dueDate ;
                           owl:maxCardinality "1"^^xsd:nonNegativeInteger
                         ] ,
